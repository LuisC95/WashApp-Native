{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/statics.tsx",
    "app/index.js",
    "app/_layout.js",
    "app/_layout.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
